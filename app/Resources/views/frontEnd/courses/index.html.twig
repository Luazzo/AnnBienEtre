{% extends "frontEnd/listing-layout.html.twig" %}
{% block titre %}Tout Les courses
{% endblock %}
{% block posts %}

    {% if  courses is iterable%}
        {%for course in  courses%}
            {{ include('Partials/big-post.html.twig',{post:{id:course.id, nom:course.name,image:'img/logo.png',description:course.description,rout:'course_show'}}) }}
        {%endfor%}
    {%else%}
        {{ include('Partials/big-post.html.twig',{post:{id:course.id, nom:course.name,image:'img/logo.png',description:course.description,rout:'course_show'}}) }}
    {%endif%}
{%endblock%}