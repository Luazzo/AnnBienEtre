{% extends "frontEnd/listing-layout.html.twig" %}
{% block titre %}Tout Les Promotions
{% endblock %}
{% block posts %}

    {% if  promotions is iterable%}
        {%for promotion in  promotions%}
            {{ include('Partials/big-post.html.twig',{post:{id:promotion.id, nom:promotion.name,image:'/Resources/public/img/logo.png',description:promotion.description,rout:'promotions_show'}}) }}
        {%endfor%}
    {%else%}
        {{ include('Partials/big-post.html.twig',{post:{id:promotion.id, nom:promotion.name,image:'/Resources/public/img/logo.png',description:promotion.description,rout:'promotions_show'}}) }}
    {%endif%}
{%endblock%}