{% extends "frontEnd/listing-layout.html.twig" %}
{% block titre %}Tout Les Promotions
{% endblock %}
{% block posts %}

    {% if  promotions is iterable%}
        {%for promotion in  promotions%}
            {{ include('Partials/big-post.html.twig',{post:{id:promotion.id, name:promotion.name,image:'img/logo.png',description:promotion.description,rout:'promotion_show'}}) }}
        {%endfor%}
    {%else%}
        {{ include('Partials/big-post.html.twig',{post:{id:promotion.id, name:promotion.name,image:'img/logo.png',description:promotion.description,rout:'promotion_show'}}) }}
    {%endif%}
{%endblock%}